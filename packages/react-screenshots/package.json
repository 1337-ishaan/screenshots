{
  "name": "react-screenshots",
  "version": "0.0.4",
  "description": "a picture clipping and graffiti tool by react",
  "main": "dist/screenshots.js",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "dev": "npm run dev:web & npm run dev:electron",
    "dev:web": "webpack-dev-server --config build/webpack.dev.web.js",
    "dev:electron": "webpack --config build/webpack.dev.electron.js",
    "build": "npm run build:web & npm run build:electron",
    "build:web": "webpack --config build/webpack.prod.web.js",
    "build:electron": "webpack --config build/webpack.prod.electron.js",
    "lint": "eslint . --ext .js,.jsx --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nashaofu/electron-screenshots.git"
  },
  "keywords": [
    "clipping",
    "graffiti",
    "react",
    "image"
  ],
  "author": "nashaofu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nashaofu/electron-screenshots/issues"
  },
  "homepage": "https://github.com/nashaofu/electron-screenshots#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "core-js": "^3.4.7",
    "normalize.css": "^8.0.1",
    "raf": "^3.4.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/runtime": "^7.7.6",
    "autoprefixer": "^9.7.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.1",
    "eslint": "^6.7.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "style-loader": "^1.0.1",
    "terser-webpack-plugin": "^2.3.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  }
}
